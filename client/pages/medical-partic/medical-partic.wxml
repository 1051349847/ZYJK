<view style='width:100%;height:20rpx;background:#e5e5e5 '></view>
<view class='med'>
  <ul>
      <block>
      <li class="on_1">
          <text  class='partic-tite'>电子病历</text>
          <text class='partic-name'>{{zj.name}}</text>
      </li>
       <li class="on_1">
          <text  class='partic-tite'>性别</text>
          <text class='partic-name'>{{zj.sex}}</text>
      </li>
       <li class="on_1">
          <text  class='partic-tite'>年龄</text>
          <text class='partic-name'>{{zj.age}}</text>
      </li>
       <li class="on_1">
          <text  class='partic-tite'>住址</text>
          <text class='partic-name'>{{zj.address}}</text>
