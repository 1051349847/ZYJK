          <text  class='partic-tite'>就诊时间</text>
          <text class='partic-name' id="partic-data">{{zj.clinic_time}}</text>
      </li>
  </ul>
</view>
<view style='width:100%;height:20rpx;background:#e5e5e5 '></view>
