.navbar__title {
  color: #999;
}

.navbar {
  display: flex;
  position: fixed;
  z-index: 500;
  top: 0;
  width: 100%;
  background: #fff;
}

.navbar__item {
  position: relative;
  display: block;
  flex: 1;
  padding: 10rpx 0;
  text-align: center;
  font-size: 0;
  height: 48rpx;
  line-height: 48rpx;
  width: 90rpx;
  /* <!-- NavBar的总高度为：height + padding-top + padding-bottom = 68rpx --> */
}

.navbar__item_on  .navbar__title {
  color: #f18d00;
}

.navbar__slider {
  /* position: absolute; */
  display: block;
  width: 90rpx;
  /* color: red; */
  content: " ";
  left: 0;
  bottom: 0;
  height: 3px;
  background-color: #16b13a;
  transition: transform 0.3s;
}

.navbar__title {
  display: inline-block;
  font-size: 32rpx;
  /* color:#333333; */
  max-width: 8em;
  text-align: center;
}

